{
  "name": "react-c3-component",
  "version": "1.3.2",
  "description": "It is a small C3 chart component.",
  "main": "lib/Chart.js",
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc npm run test",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "lint": "eslint ./ && echo 'lint succesed'",
    "prebuild": "npm run test",
    "build": "babel src --out-dir lib --ignore *.test.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wuct/react-c3-component"
  },
  "keywords": [
    "react"
  ],
  "author": "wuct",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wuct/react-c3-component/issues"
  },
  "homepage": "https://github.com/wuct/react-c3-component",
  "ava": {
    "files": [
      "src/Chart.test.js"
    ],
    "source": [
      "src/Chart.js"
    ],
    "failFast": true,
    "require": [
      "babel-register",
      "./testSetup"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "c3": "^0.4.11",
    "codecov": "^1.0.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.4.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^5.0.1",
    "expect": "^1.20.1",
    "jsdom": "^9.2.1",
    "nyc": "^6.4.4",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "peerDependencies": {
    "c3": "^0.4.0",
    "react": "^0.14.7 || ^0.15.0",
    "react-dom": "^0.14.7 || ^0.15.0"
  }
}
